// lock-acquire-in-split-case.pdl
pipe test22(input: uint<32>, randomBool: bool, randomBool2: bool)[rf: int<32>[5]<c,s>(Queue)] {
    start(rf);
    split {
        case: randomBool {
            reserve(rf);
        }
        case: randomBool2 {
            reserve(rf);
        }
        case: (input{0:0} == u1) {
            reserve(rf);
        }
        default: {
            reserve(rf);
        }
    }
    end(rf);
    ---
    block(rf);
    int<32> x <- rf[input{4:0}];
    release(rf);
    ---
    call test22(input, randomBool, randomBool2);
}

circuit {
    r = memory(int<32>, 5);
}