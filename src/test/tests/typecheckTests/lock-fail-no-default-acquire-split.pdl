// lock-fail-no-default-acquire-split.pdl
pipe test21(input: int<32>, randomBool: bool, randomBool2: bool)[rf: int<32>[5]] {
    start(rf);
    split {
        case: randomBool {
            acquire(rf);
        }
        case: randomBool2 {
            acquire(rf);
        }
        case: (input{0:0} == 1) {
            acquire(rf);
        }
    }
    release(rf);
    end(rf);
    call test21(input, randomBool, randomBool2);
}

circuit {
    r = memory(int<32>, 5);
}