//Expected Success
pipe test17(input: int<32>, randomBool: bool, randomBool2: bool)[rf: int<32>[5]<c2,s>(Queue)] {
    bool a = true;
    bool b = false;
    start(rf);
    if (randomBool == a) {
        if (randomBool2 == b){
            reserve(rf);
        }
    }
    ---
    if (randomBool == a) {
        if (randomBool2 == b){
	    block(rf);
            release(rf);
        }
    }
    end(rf);
    call test17(input, randomBool2, randomBool);
}


