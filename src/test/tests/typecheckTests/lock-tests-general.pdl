//Expected success since reserve is before branch
pipe test8(input: int<32>)[rf: int<32>[5]<a,a>(Queue)] {
    start(rf);
    if (input{0:0} == 1) {
        ---
        reserve(rf);
	---
	block(rf);
        int<32> out <- rf[u0<5>];
    } else {
        ---
	reserve(rf);
	---
	block(rf);
        int<32> out <- rf[u1<5>];
    }
    call test8(input);
    ---
    rf[u2<5>] <- out;
    release(rf);
    end(rf);
    ---
}
