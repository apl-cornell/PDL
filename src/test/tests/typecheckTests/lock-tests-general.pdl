// Expected Success
pipe test2(input: int<32>)[rf: int<32>[5]<c2,s>(Queue)] {
    start(rf);
    if (input{0:0} == 1) {
        reserve(rf);
	---
	block(rf);
        release(rf);
    }
    end(rf);
    call test2(input);
}
//Expected Success
pipe test4(input: int<32>)[rf: int<32>[5]<c2,s>(Queue)] {
    start(rf);
    if (input{0:0} == 1) {
	reserve(rf);
	---
	block(rf);
        int<32> arg <- rf[cast(input{4:0}, uint<5>)];
        release(rf);
    } else {
        ---
	reserve(rf);
	---
        block(rf);
        ---
        release(rf);
    }
    end(rf);
    call test4(input);
}
