pipe test(input: int<32>)[rf: int<32>[5]] {
    start(rf);
    acquire(rf);
    end(rf);
    if (input{0:0} == 1) {
        rf[input{4:0}] <- input;
        ---
        int<32> y = input;
        ---
        int<32> z = input;
    } else {
        int<32> x = input;
    }
    release(rf);
    ---
    call test(input + 1<32>);
}

circuit {
    rf = regfile(int<32>, 5);
}