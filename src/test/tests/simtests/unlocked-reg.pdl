// unlocked-reg.pdl
//Expected Success
pipe unlock(tinput: int<32>)[rf: int<32>[0]<c,s>]: bool {
    spec_check();
    s <- speccall unlock(tinput + 1<32>);
    ---
    start(rf);
    int<32> x = rf[] + tinput;
    end(rf);
    ---
    spec_barrier();
    print(x);
    if (x < 10) {
        verify(s, x);
    } else {
      invalidate(s);
      output true;
    }
}

circuit {
      r = register(int<32>, 1);
      un = new unlock[r];
      call un(0<32>);
}