pipe test(pc: uint<32>)[rf: int<32>[5]<a,a>(LSQ)]: bool {
    uint<5> rs1 = pc{4:0};
    uint<5> rd = rs1 - u1<5>;
    bool done = pc >= u100<32>;
    if (!done) {
        call test(pc + u1<32>);
	start(rf);
	reserve(rf[rs1], R);
        reserve(rf[rd], W);
        end(rf);
        ---
        block(rf[rs1]);
        int<32> a1 <- rf[rs1];
        ---
        int<32> arg = 1<32> + a1;
        release(rf[rs1]);
        ---
        block(rf[rd]);
        print(arg);
        rf[rd] <- arg;
        release(rf[rd]);
    }
    ---
    if (done) {
       output(true);
    }
}

circuit {
   m = memory(int<32>, 5);
   lq = LSQ(m);
   t = new test[lq];
   call t(u1<32>);
}