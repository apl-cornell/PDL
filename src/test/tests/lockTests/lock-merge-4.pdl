pipe ex4(in: bool)[rf: int<32>[5]] {
    start(rf);
    uint<5> addr = u1<5>;
    reserve(rf[addr],R);
    ---
    uint<5> addr2 = u2<5>;
    bool cond2 = true;
    if (cond2) {
        reserve(rf[addr2],R);
    }
    end(rf);
    if (in) {
        ---
        if (in) {
            block(rf[addr]);
        }
        ---
        if (cond2) {
                block(rf[addr2]);
                release(rf[addr2]);
        }
        ---
    } else {
        ---
        block(rf[addr]);
        ---
    }
    release(rf[addr]);
    --- //TODO remove this separation, currently can't compile due to concurrent release w/ only 1 release port
    if ((!in) && cond2) {
        block(rf[addr2]);
        release(rf[addr2]);
    }
    int<1> x = 1<1>;
}

circuit {
    t1 = memory(int<32>, 5);
    r1 = Queue(t1);
    e = new ex4[r1];
    call e(true);
}