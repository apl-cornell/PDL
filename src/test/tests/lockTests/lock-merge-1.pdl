pipe ex1(in: bool)[r1: int<32>[5], r2: int<32>[5]] {
    start(r1);
    start(r2);
    int<5> addr = 1<5>;
    if (in) {
        reserve(r1[addr], WRITE);
    } else {
        reserve(r2[addr], WRITE);
    }
    end(r1);
    end(r2);
    ---
    if (in) {
        block(r1[addr]);
        r1[addr] <- 32<32>;
        release(r1[addr]);
    }
    ---
    if (!in) {
        block(r2[addr]);
        r2[addr] <- 32<32>;
        release(r2[addr]);
    }
    print(in);
}

circuit {
    r1 = memory(int<32>, 5);
    r2 = memory(int<32>, 5);
    e = new ex1[r1, r2];
    call e(false);
}