// branch-1.pdl
pipe test(inarg: int<32>)[rf: int<32>[5]] {
    start(rf);

    if (inarg{0:0} == 1) {
        rf[cast(inarg{4:0},uint<5>)] <- inarg;
        ---
        int<32> y = inarg;
        ---
        int<32> z = inarg;
    } else {
        int<32> x = inarg;
    }
    end(rf);    
    ---
    call test(inarg + 1<32>);
}

circuit {
    rf = regfile(int<32>, 5);
}