pipe test(inarg:int<32>)[rf:int<32>[5]<a, a>] {
  start(rf);
  int<2> cond = inarg{1:0};
  split {
    case: cond == 0<2> {
        int<5> addr = 0<5>;
        
    }
    case: cond == 1<2> {
        int<5> addr = 1<5>;
        
    }
    case: cond == 2<2> {
        int<5> addr = 2<5>;
        
    }
    default: {
        int<5> addr = 3<5>;
        
    }
  }
  reserved(rf[addr],W);
  acquired(rf[addr],W);
  end(rf);
  
  ---
  rf[addr] <- 0<32>;
  released(rf[addr]);
  
  ---
  call test(inarg + 1<32>);
  
