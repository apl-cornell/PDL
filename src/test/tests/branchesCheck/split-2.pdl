pipe test(inarg: int<32>)[rf: int<32>[5]] {
    start(rf);
    int<2> cond = inarg{1:0};
    split {
        case: (cond == 0<2>) {
            uint<5> addr = u0<5>;
        }
        case: (cond == 1<2>) {
            uint<5> addr = u1<5>;
        }
        case: (cond == 2<2>) {
            uint<5> addr = u2<5>;
        }
        default: {
            uint<5> addr = u3<5>;
        }
    }
    acquire(rf[addr], W);
    end(rf);
    ---
    rf[addr] <- 0<32>;
    release(rf[addr]);
    ---
    call test(inarg + 1<32>);
}

circuit {
    rf = regfile(int<32>, 5);
}